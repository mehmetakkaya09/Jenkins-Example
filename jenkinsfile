pipeline {

  agent any

  stages {

    stage("commit"){
      steps{
        echo "code commited"
      }
    }

    stage("build"){
      steps{
        echo "code built"
      }
    }

    stage("dev-unit"){
      steps{
        echo "unit test began"
      }
    }

    stage("dev-integration"){
      steps{
        echo "integration test began"
      }
    }

    stage("qa-smoke"){
      steps{
        echo "smoke test started"
      }
    }

    stage("qa-regression"){
      steps{
        echo "regression test started"
      }
    }

    stage("report"){
      steps{
        echo "test report is generated"
      }
    }

  }


}
